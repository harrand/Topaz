mem_zero ::= func(memory : u8 mut& weak, size : u64) -> v0
{
	counter : u64 mut := 0;
	while(counter < size)
	{
		deref(memory at counter) = 0;
		counter = counter + 1;
	}
};

mem_copy ::= func(dst : u8 mut& weak, src : u8& weak, size : u64) -> v0
{
	counter : u64 mut := 0;
	while(counter < size)
	{
		deref(dst at counter) = deref(src at counter);
		counter = counter + 1;
	}
};

